{
  "name": "cra-config-create-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start --progress",
    "dev": "react-app-rewired start --progress",
    "dev-mock": "cross-env REACT_APP_MOCK=true npm run dev",
    "build": "react-app-rewired build",
    "analyze": "react-app-rewired build --analyze",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "format-code": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.jsx\"",
    "lint-staged": "lint-staged",
    "precommit-msg": "echo 'ðŸ‰ Start pre-commit checks...' && exit 0",
    "postinstall": "node node_modules/husky/lib/installer/bin install"
  },
  "dependencies": {
    "@rematch/core": "^1.2.0",
    "@rematch/immer": "^1.1.0",
    "@rematch/loading": "^1.1.3",
    "antd": "^3.23.2",
    "history": "^4.10.1",
    "moment": "^2.24.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.1",
    "react-router": "^5.0.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.1",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.12.1",
    "customize-cra": "^0.6.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.5",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.5",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.18.2",
    "prettier-eslint-cli": "^5.0.0",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.3",
    "react-hot-loader": "^4.12.13",
    "webpack-bundle-analyzer": "^3.5.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,less}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "pre-commit": [
    "precommit-msg",
    "lint-staged"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit-msg && lint-staged"
    }
  },
  "theme": "./src/theme.js",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
